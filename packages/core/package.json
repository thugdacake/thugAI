    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"

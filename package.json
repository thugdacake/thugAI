{
  "name": "cerebro-operante",
  "version": "1.0.0",
  "description": "Framework de Produção Universal para desenvolvimento de software",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "docs": "chmod +x scripts/generate-docs.sh && ./scripts/generate-docs.sh",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "validate": "npm run lint && npm run test",
    "prepare": "husky install"
  },
  "keywords": [
    "framework",
    "automation",
    "ci-cd",
    "development",
    "workflow"
  ],
  "author": "Cérebro Operante",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0",
    "dotenv": "^16.0.0",
    "commander": "^9.0.0",
    "chalk": "^4.1.0",
    "inquirer": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0"
  }
} 